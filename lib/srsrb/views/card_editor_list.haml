%div#card-editor-list-page.page
  %div.row
    %div.large-12.columns
      %table
        %thead
          %tr
            %th Question
            %th Answer
        %tbody
          - cards.each do |card|
            %tr.card
              %td
                %a{href: card.id.to_guid}
                  &= card.question
              %td
                %a{href: card.id.to_guid}
                  &= card.answer
